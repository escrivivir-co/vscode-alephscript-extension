  "ui": [
    {
      "id": "console-demo",
      "name": "Demo Console UI",
      "type": "custom",
      "enabled": true,
      "config": {
        "isPrimary": true,
        "autoStart": true,
        "port": 8080
      }
    },
    {
      "id": "web-demo",
      "name": "Demo Web UI",
      "type": "html5",
      "enabled": true,
      "config": {
        "port": 8081,
        "autoStart": false
      }
    }
  ]
